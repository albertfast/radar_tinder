{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "YARN_IGNORE_ENGINES": "1"
      },
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "adhoc": {
      "developmentClient": true,
      "credentialsSource": "local",
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview": {
      "credentialsSource": "remote",
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "credentialsSource": "remote",
      "distribution": "store",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
